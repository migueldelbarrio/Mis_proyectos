
<% var j = 0 || users.length; %>
<h1>Número de usuarios registrados: <%= j %></h1>



<table>
<% for(i=0; i<j; i++){ %>
<tr> 

	<td>ID:<%= users[i].id %> </td><td><a href="/users/<%= users[i].id %>"><%= users[i].nombre %></a></td><td><%= users[i].pass %></td><td><a href="/users/<%= users[i].id %>/edit">Editar</a></td<td><form method="post" action="users/<%= users[i].id %>?_method=delete" ><button type="submit" onClick="return confirm('¿Seguro que desea borrarla?');">Borrar </button></form></td>

</tr>
<% } %>

</table>
<br>

<a href="/users/new">Registrar un nuevo usuario</a>